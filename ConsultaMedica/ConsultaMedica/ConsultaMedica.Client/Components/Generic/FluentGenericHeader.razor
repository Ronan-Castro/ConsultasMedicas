<MudPaper Elevation="25">
    <MudToolBar >
        @Title
        <ButtonAddGeneric Href="@Href">
            @ButtonText
        </ButtonAddGeneric>
    </MudToolBar>
</MudPaper>


@code {
    [Parameter] public string Title { get; set; }
    [Parameter] public string ButtonText { get; set; }
    [Parameter] public string Href { get; set; }
}
